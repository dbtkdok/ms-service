		<step PID="TSY0063_02" service="commService" query="chkTblCodeActs" method="query4Data" chkKey="codeType|nationCD|flag" caseDoRun="Before" getVar="existCnt" caseRET="Choice">
			<caseDo service="commService" subQuery1="insTblCodesOne" subQuery2="updTblCodesList" subMethod="query4Update" execMode="auto">
				<caseIF><![CDATA[(existCnt==0?"subQuery1":"subQuery2")]]></caseIF>
			</caseDo>
		</step>
	</proc>

	<proc>
		<step PID="TSY0064" service="commService" query="chkTblCodeType" method="query4Update1" caseRun="Before" getVar="cnt" caseRET="Choice" 
			getParams="codeType|codeTypeNM|nationCD|flag">
			<caseDo service="commService" subQuery1="continue" subQuery2="throw" subMethod="none" execMode="One" throwMsg="코드유형이 이미 존재합니다.">
				<caseIF><![CDATA[(cnt==0?"continue":"throw")]]></caseIF>
			</caseDo>
		</step>    
		<step PID="TSY0064_01" service="commService" query="insTblCodesList" method="update4sub"/>
	</proc>	

	<proc><step PID="TSY0065"  service="commService" query="delTblCodeList" method="query4Updates"/></proc>
	
