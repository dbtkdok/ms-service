	<proc><step PID="TSY0062"  service="commService" query="chkTblCodeType"  method="query4Data"/></proc>
	
	<proc>
		<step PID="TSY0063"    service="commService" query="updTblCodes4One" method="query4Update" getParams="codeType|codeTypeNM|nationCD"/>
