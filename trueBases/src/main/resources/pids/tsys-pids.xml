	<proc>
		<step PID="TSY0083" service="commService" query="insTblServices4One" method="query4Updates" getKey="procID|procRemk|pidsXml" 
			caseRun="Before" getVar="subQuery1|subQuery2" caseRET="Boolean" 
			getParams="stepUID|stepSVC|subPrevQuery|subPrevMethod|subQuery1|subQuery2|subMethod|subPostQuery|subPostMethod|caseIF|throwMsg|execMode|sortORD|dbSTS">
			<caseDo service="commService" subQuery1="insTblCaseDo4One" subMethod="query4Update" execMode="Auto">
				<caseIF><![CDATA[('subQuery1'!='' || 'subQuery2'!='')]]></caseIF>
			</caseDo>
		</step>
	</proc>
	<proc>
		<step PID="TSY0084" service="commService" prevQuery="delTblServicesByprocID" prevMethod="query4Update"
			query="insTblServices4One" method="query4Updates" getKey="procID|procRemk|pidsXml"
			caseRun="Before" getVar="subQuery1|subQuery2" caseRET="Boolean" 
			getParams="stepUID|stepSVC|subPrevQuery|subPrevMethod|subQuery1|subQuery2|subMethod|subPostQuery|subPostMethod|caseIF|throwMsg|execMode|sortORD|dbSTS">
			<caseDo service="commService" subPrevQuery="delTblCaseDoBystepUID" subQuery1="insTblCaseDo4One" subMethod="query4Update" execMode="Auto">
-  				<caseIF><![CDATA[('subQuery1'!='' || 'subQuery2'!='')]]></caseIF>
			</caseDo>
		</step>
	</proc>
	<proc>
		<step PID="TSY0089" service="commService" query="delTblServicesBystepUID" method="query4Updates" caseDoRun="Before" getParams="stepUID">
			<caseDo service="commService" subQuery1="delTblCaseDoBystepUID" subMethod="query4Update" execMode="Auto" />
		</step>
	</proc>
	
		<!-- CodesList management -->
