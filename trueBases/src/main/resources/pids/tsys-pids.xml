	<proc><step PID="TSY0023"  service="commService" query="insTblTableList" method="query4Updates"/></proc>
	<proc>
		<step PID="TSY0024"    service="commService" query="updTblTable4One" method="query4Update"/>
		<step PID="TSY0024_01" service="commService" query="delTblTableColumn" method="update4sub"/>
		<step PID="TSY0024_02" service="commService" query="chkTblTableActs" method="query4Data" 
			chkKey="tblUID" caseDoRun="Before" getVar="existCnt" caseRET="Choice">
			<caseDo service="commService" subQuery1="insTblTableOne" subQuery2="updTblTable4ActsOne" subMethod="query4Update" execMode="auto">
				<caseIF><![CDATA[(existCnt==0?"subQuery1":"subQuery2")]]></caseIF>
			</caseDo>
		</step>
	</proc>
	<proc><step PID="TSY0029"  service="commService" query="delTblTableList" method="query4Updates"/></proc>

